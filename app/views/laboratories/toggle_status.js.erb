$(function () {
  var $laboratoryLock = $('.laboratory-lock');

  <% if @laboratory.status == "active" %>
    $laboratoryLock.removeClass('fa-unlock');
    $laboratoryLock.addClass('fa-lock');
  <% else %>
    $laboratoryLock.removeClass('fa-lock');
    $laboratoryLock.addClass('fa-unlock');
  <% end %>
});