<div id="employee-assign" class="tabcontent">
  <table class="table table-no-bordered table-hover">
    <thead>
      <th>Nombre</th>
      <th>Categoría</th>
      <th>Método</th>
      <th>Acreditado</th>
      <th>Asinación</th>
    </thead>
    <tbody>
      <% @service.sample_processeds.each do |sample_processed| %>        
          <td><%= sample_processed.description %></td>                          
          <td><%= sample_processed.sample_category.name %></td>                          
          <td><%= sample_processed.sample_method.name %></td>                          
          <td><%= sample_processed.sample_method.accredited? %></td>                          
          <td><%= f.select :id, @employees, {}%></td>        
      <% end %>
    </tbody>
  </table>  
</div>