<%= form_tag search_services_path, class: "validate", method: :get, remote: true do %>
  <div class="row form-group">
    <div class="col-sm-6">
      <div class="col-sm-4">
        <%= label_tag :start_date, nil, class: "control-tag" %>
      </div>
      <div class="col-sm-8">
        <%= text_field_tag :start_date, nil, class: 'form-control datepicker' %>
      </div>
    </div>
    <div class="col-sm-6">
      <div class="col-sm-4">
        <%= label_tag :end_date, nil, class: "control-tag" %>
      </div>
      <div class="col-sm-8">
        <%= text_field_tag :end_date, nil, class: 'form-control datepicker' %>
      </div>
    </div>
  </div>
  <div class="row form-group">
    <div class="col-sm-12">
      <div class="col-sm-12">
        <%= submit_tag "Search", class:"btn btn-default pull-right" %>
      </div>
    </div>
  </div>
<% end %>