<% service.sample_processeds.each do |sample_processed| %>
  <div class="sample-fields">
    <div class="form-group row">
      <%= f.label :Codigo_cliente, class: "col-md-3 col-fom-label" %>
       <div class="col-md-9">
          <%= sample_processed.pucp_code %>
      </div>
    </div>
  </div>      
  <div class="form-group row">
    <%= f.label :Codigo_pucp, class: "col-md-3 col-fom-label" %>
     <div class="col-md-9">
        <%= sample_processed.client_code %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :Categoria, class: "col-md-3 col-fom-label" %>
     <div class="col-md-9">
        <%= sample_processed.sample_category.name %>
    </div>
  </div>     
  <div class="form-group row">
    <%= f.label :Descripcion, class: "col-md-3 col-fom-label" %>
     <div class="col-md-9">
        <%= sample_processed.description %>
    </div>
  </div> 
  <div class="form-group row">
    <%= f.label :Valores, class: "col-md-3 col-fom-label" %>
  </div>
  <div class="form-group row">
    <div class="col-md-9 offset-md-3">
      <div class="form-group row">
        <div class="col-md-6">Descripcion</div>
        <div class="col-md-1">Valor</div>
      </div>
      <div class="samples-values nested-fields form-group row">  
        <% sample_processed.sample_features.each do |sample_feature| %>
          <div class="col-md-6">
            <%= sample_feature.description %>
          </div>
          <div class="col-md-1">
            <%= sample_feature.value %>
          </div>
        <% end %>
      </div>
    </div>
  </div>     
<% end %>