<div class="card">
  <% if @work_order.to_work? %>
    <div class="tab">
      <button class="tablinks active" onclick="openSection(event, 'information')">Información Cliente</button>
      <button class="tablinks" onclick="openSection(event, 'custody')">Cadena de Custodia</button>
      <button class="tablinks" onclick="openSection(event, 'report')">Reporte</button>
    </div>
    <%= render 'work_orders_to_work'%>
  <% else %>
     <div class="tab">
      <button class="tablinks active" onclick="openSection(event, 'information')">Información Cliente</button>
      <button class="tablinks" onclick="openSection(event, 'custody')">Cadena de Custodia</button>
      <button class="tablinks" onclick="openSection(event, 'report')">Reporte - Revisión </button>
    </div>
    <%= render 'work_orders_to_rework'%>
  <% end %>
</div>
