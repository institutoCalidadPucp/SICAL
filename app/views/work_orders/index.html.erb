      <div class="content">
        <div class="header card-header-text sical-card-header">
          <h1 class="sical-card-title">
            Ordenes de trabajo
          </h1>
        </div>
        <div class="sical-content">
          <div class="row">
            <div id="unread-card" class="col-md-4 card">
              <%= render 'work_orders', work_orders: @work_orders_to_work, title: "Ordenes por Realizar" %>
            </div>
            <div id="read-card" class="col-md-4 card">
              <%#= render 'work_orders', work_orders: @work_orders_to_check, title: "Ordenes por Revisar" %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
