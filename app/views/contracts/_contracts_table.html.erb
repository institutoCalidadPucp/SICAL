<div class="content">
        <div class="material-datatables">
          <table id="contract_table" class="table table-no-bordered table-hover" >
            <thead>
              <tr>
                <th>Código</th>
                <th>Código de Cotización</th>
                <th>Laboratorio</th>
                <th>Fecha de Registro</th>
                <th>Estado</th>
                <th>Acciones</th>
              </tr>
            </thead>
          </table>
        </div>
</div>


<script>
  $(document).on('ready', function() {
    $('#contract_table').DataTable({
        "pagingType": "full_numbers",
        "lengthMenu": [ 
            [10, 25, 50, -1],
            [10, 25, 50, "All"]
        ],
        responsive: true,
        destroy: true,
        language: {
            search: "_INPUT_",
            searchPlaceholder: "Buscar Contratos",
        },
    });
  })
</script>
