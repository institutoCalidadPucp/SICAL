          <h1 class="sical-card-title">
            Cadena de Custodia
          </h1>
        </div>
        <div class="sical-content">
          <div class="row">
            <div id="unread-card" class="col-md-6 card">
              <%= render 'unclassified_services_card', services_unclassified: @services_unclassified, title: "Servicios sin Custodia" %>
            </div>
            <div id="read-card" class="col-md-6 card">
              <%= render 'classified_services_to_check_card', services_classified_to_check: @services_classified_to_check, title: "Custodia por Revisar" %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
