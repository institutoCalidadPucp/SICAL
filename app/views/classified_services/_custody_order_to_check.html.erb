 <%= form_for @custody_order, url: work_check_update_classified_service_path, :wrapper => false,  method: :put do |f| %>
  <div id="information" class="tabcontent" style="display: block;">
        <h3>Información</h3>
          <%= render 'show_basic_information', service: @service, f: f %>
  </div>
      
  <div id="custody" class="tabcontent">
        <h3>Revisión</h3>
        <%= render 'custody_check', sample: @custody_order.sample_processed, f: f %>
  </div>

  <div class="form-group">
    <div class="text-center save-worker">
      <%= link_to "Cancelar", classified_services_path, class: "btn btn-primary sical-btn-cancel" %>
      <%= f.submit "Asignar", class: "btn btn-primary", data: {disable_with: 'Please Wait...'}  %>
    </div>
  </div>
  <% end %>
